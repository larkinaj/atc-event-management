{
    "compilerOptions": {
      "baseUrl": ".",
      "module": "es2022",
      "target": "es5",
      "strict": true,
      "sourceMap": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "outDir": "./dist"
    },
    "exclude": [
      "node_modules"
    ]
  }